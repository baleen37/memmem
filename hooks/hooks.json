{
  "description": "Auto-install dependencies on first use",
  "hooks": {
    "SessionStart": [
      {
        "matcher": "*",
        "hooks": [
          {
            "type": "command",
            "command": "bash ${CLAUDE_PLUGIN_ROOT}/hooks/install-deps.sh",
            "timeout": 120
          }
        ]
      }
    ]
  }
}
